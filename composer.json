{
  "name": "uistd/git",
  "description": "UI Service 简单的PHP Git工具，主要用于代码自动化生成之后，自动将代码push入库",
  "keywords": [
    "ui service",
    "uistd",
    "git"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder and project lead"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "uistd/common": "3.*",
    "uistd/logger": "2.*"
  },
  "autoload": {
    "psr-4": {
      "UiStd\\Git\\": "src"
    }
  },
  "minimum-stability": "stable",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  },
  "config": {
    "secure-http": false
  }
}